    "test:lint": "./node_modules/mocha/bin/_mocha --compilers ts-node/register test/tslint.ts",
    "test:fix-lint": "./node_modules/.bin/tslint --fix -p . -c tslint.json -t \"stylish\" -e \"node_modules/**/*.ts\" -e \"**/*.d.ts\" \"private/**/*.ts\" \"public/**/*.ts\" \"public/**/*.vue\" \"test/**/*.ts\""
  },
